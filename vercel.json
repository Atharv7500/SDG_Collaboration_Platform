{
    "builds": [
        {
            "src": "backend/sdg_backend/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "buildCommand": "sh build_files.sh",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/sdg_backend/wsgi.py"
        }
    ]
}
